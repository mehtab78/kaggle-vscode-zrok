# Add this to ~/.ssh/config
# (connect.py does this automatically)

# Kaggle Remote Connection via zrok private tunnel
Host kaggle_client
    HostName 127.0.0.1
    User root
    Port 9191
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ServerAliveInterval 60
    ServerAliveCountMax 3
